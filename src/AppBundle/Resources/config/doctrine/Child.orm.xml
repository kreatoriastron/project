<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Child" table="child">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="string" column="surname" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parentId" type="integer" column="parent_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="groupId" type="integer" column="group_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="schoolId" type="integer" column="school_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="classDigit" type="integer" column="class_digit" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="classLetter" type="string" column="class_letter" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
